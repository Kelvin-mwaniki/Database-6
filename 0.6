-- Create a new database named UniversityDB
CREATE DATABASE UniversityDB;

-- Use the UniversityDB database
USE UniversityDB;

-- Create the Students table
CREATE TABLE Students (
  StudentID INT PRIMARY KEY,
  FirstName VARCHAR(50),
  LastName VARCHAR(50),
  Age INT,
  Major VARCHAR(50)
);

